    "timeline": {
      "title": "Timeline",
      "type": "BarChart",
      "height": "192px",
      "controls": {
        "granularity": {
          "type": "select",
          "options": ["Auto (By Hour)", "Daily", "Weekly"],
          "default": "hourly"
        }
      }
    },

    "summaryTable": {
      "title": "Summary",
      "controls": [
        {
          "type": "button",
          "text": "EXPORT CSV",
          "icon": "Download",
          "action": "exportToCSV"
        },
        {
          "type": "button",
          "icon": "Settings",
          "action": "showSettings"
        }
      ],
      "tabs": [
        "Campaign",
        "Publisher", 
        "Target",
        "Buyer",
        "Dialed #",
        "Number Pool",
        "Date",
        "Duplicate"
      ],
      "extraTab": {
        "text": "Tags",
        "icon": "ChevronDown",
        "action": "showTagsDialog"
      }
    }
  },

